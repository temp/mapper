{
    "name": "temp/mapper",
    "authors": [
        {
            "name": "Stephan Wentz",
            "email": "stephan@wentz.it"
        }
    ],
    "require": {
        "php": "^7.3",
        "thecodingmachine/safe": "^0.1.15"
    },
    "require-dev": {
        "brainbits/phpcs-standard": "^3.0@dev",
        "phpunit/phpunit": "^8.0",
        "thecodingmachine/phpstan-safe-rule": "^0.1.3",
        "jangregor/phpstan-prophecy": "^0.4.0",
        "phpstan/phpstan-phpunit": "^0.11.0"
    },
    "autoload": {
        "psr-4": {
            "Mapper\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "MapperTests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
